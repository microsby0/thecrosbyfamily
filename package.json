{
  "name": "thecrosbyfamily",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "prop-types": "^15.7.2",
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "@testing-library/react": "^9.3.2",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "server": "node server.js",
    "server:local": "export API_URL=https://localhost:8080; nodemon server.js",
    "server:dev": "export API_URL=https://localhost:8080; nodemon server.js",
    "local": "run-p server:local start",
    "dev": "run-p server:dev start",
    "lint": "npx eslint 'src/**'",
    "lint:fix": "npx eslint 'src/**' --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretest": "npx eslint 'src/**'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8080"
}
